rbspCom.AddVariable( Runner_Port_Input_Final )
rbspCom.AddVariable( Runner_Stb_Input_Final )
rbspCom.AddVariable( Keel_Press_Stem_Input_Final )
rbspCom.AddVariable( Keel_Press_Barrel_Input_Final )
rbspCom.AddVariable( Foil_Active_Rake_Final )
rbspCom.AddVariable( KeelAngle_Input_Final )
rbspCom.AddVariable( Heel_Input_Final )
rbspCom.AddVariable( Outrigger_Active_Final )
rbspCom.AddVariable( Runner_Active_Final )
rbspCom.AddVariable( Foil_Stb_peak_Final )
