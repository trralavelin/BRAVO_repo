new TRBSPUdpServer ( rbspCom , 5242, All )
rbspCom.SetKeepAlive( 300 )
rbspCom.AddFinalVars()
rbspCom.AddVariable( gpsCOGtrue )
rbspCom.AddVariable( LWYHEELi_damped )
rbspCom.AddVariable( E0_Input_Final )
rbspCom.AddVariable( E2_Input_Final )
rbspCom.AddVariable( E3_Input_Final )
rbspCom.AddVariable( E4_Input_Final )
rbspCom.AddVariable( Outrigger_Port_Input_Final )
